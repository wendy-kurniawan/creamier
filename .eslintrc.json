{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base", "plugin:codeceptjs/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/no-extraneous-dependencies": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "linebreak-style": "off",
    "class-methods-use-this": "off",
    "no-new": "off"
  }
}
